algoritmo "EXEMPLO DE VETORES"

var
EXP1:VETOR[0..5] DE INTEIRO
EXP2:VETOR[1..5] DE INTEIRO
i:inteiro
inicio
para i de 1 ate 5 faca
   EXP1[i]<-i
   EXP2[i]<-i
fimpara
escreval
escreval("Exp1: ")
para i de 1 ate 5 faca
   escreva(EXP1[i])
fimpara
escreval
escreval("Exp2: ")
para i de 1 ate 5 faca
   escreva(EXP2[i])
fimpara
fimalgoritmo
