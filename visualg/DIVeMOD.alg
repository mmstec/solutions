algoritmo "EXEMPLO DE MOD E DIV"
var
X,Y:INTEIRO
inicio
X<-10
Y<-5
ESCREVAL("EXP.10 DIVIDIDO POR 5 = 2 (RESTO 0)")
ESCREVAL("EXEMPLO X DIV Y: ",X DIV Y)
ESCREVAL("EXEMPLO X MOD Y: ",X MOD Y)

SE(X MOD Y=0) ENTAO
   ESCREVAL(X," É PAR")
SENAO
   ESCREVAL(X," NÃO É PAR")
FIMSE

SE(X>0) E (X<20) ENTAO
   ESCREVAL(X," ESTA ENTRE 0 E 20")
SENAO
   ESCREVAL(X," NÃO ESTA ENTRE 0 E 20")
FIMSE

fimalgoritmo
